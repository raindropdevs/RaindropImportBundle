{
    "name": "raindrop/import-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Raindrop ImportBundle",
    "keywords": ["import", "export"],
    "homepage": "https://github.com/raindropdevs",
    "license": "MIT",
    "authors": [
        {
            "name": "Raindrop Dev Team",
            "homepage": "https://github.com/raindropdevs/RaindropImportBundle/graphs/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3.3",
        "symfony/framework-bundle": ">=2.1.0,<2.3",
        "symfony/finder": "2.1.*@dev",
        "symfony/console": "2.1.*@dev",
        "symfony/yaml": "2.1.*@dev",
        "doctrine/orm": ">=2.2.3,<2.4-dev",
        "symfony/monolog-bundle": ">=2.1,<3"
    },
    "require-dev": {
        "symfony/class-loader": "2.1.x@dev",
        "symfony/validator": "2.1.x@dev"
    },
    "autoload": {
        "psr-0": { "Raindrop\\ImportBundle": "" }
    },
    "target-dir": "Raindrop/ImportBundle"
}
